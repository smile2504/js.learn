console.log(2>1)
console.log(2<1)
console.log(2>=1)
console.log(2==1)
console.log(2!=1)

/*********** Avoid these confusings ******************/

console.log("2">1)
console.log("2"<1)
console.log("2">=1)
console.log("2"==1)
console.log("2"!=1)

console.log(null > 1)
console.log(null < 1)
console.log(null >= 1)
console.log(null == 1)
console.log(null != 1)

console.log(undefined > 1)
console.log(undefined < 1)
console.log(undefined >= 1)
console.log(undefined == 1)
console.log(undefined != 1)

console.log("2"===1)
